{
    "extends": [
        "./node_modules/javascript-style-guide/.eslintrc"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-param-reassign": ["error", { "props": false }],
        "no-underscore-dangle": ["error", { "allow": ["_get", "_set"] }],
        "no-unused-vars": ["warn"]
    },
    "globals": {
        "Backbone": true,
        "Marionette": true,
        "_": true,
        "$": true
    },
    "env": {
        "browser": true
    }
}
